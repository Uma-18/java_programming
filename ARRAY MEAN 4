import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int size = sc.nextInt();
        int arr[] = new int[size],sum=0;
        for (int i = 0; i < size; i++) {
            arr[i] = sc.nextInt();
            sum +=arr[i];
        }
         float ave=(float)sum / size;
        System.out.printf("Array Mean Value is %.2f",ave);
    }
}
